{
  "schemaVersion": 1,
  "id": "gumballglient",
  "version": "${version}",
  "name": "GumballClient",
  "description": "GumballClient by GumballGardas",
  "authors": [
    "GumballGardas"
  ],
  "contact": {
    "repo": "https://github.com/GumballGardas/Numby-hack",
    "sources": "https://github.com/GumballGardas/Numby-hack",
    "issues": "https://github.com/GumballGardas/Numby-hack/issues"
  },
  "icon": "assets/numby-hack/textures/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "GumballGardas.GumballClient.GumballClient"
    ]
  },
  "mixins": [
    "GumballClient.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "0,247,255"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": ">=0.5.5"
